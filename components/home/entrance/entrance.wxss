.s-entrance {
  width: 100%;
  background:#fff;
  margin-bottom:10rpx;
}

.s-entrance-head {
  width: 100%;
  height:78rpx;
  line-height:78rpx;
  display:flex;
  align-content:center;
  justify-content:space-between;
  font-size:30rpx;
  box-sizing:border-box;
  padding:0 30rpx;
}

.s-entrance-head-title {
  font-weight:bold;
}

.s-entrance-head-more {
  width: 200rpx;
  display:flex;
  align-items:center;
  justify-content:flex-end;
  font-size:24rpx;
}

.s-entrance-head-more:after {
  content:" ";
  width: 10rpx;
  height:10rpx;
  border:1px solid transparent;
  border-top-color:#000;
  border-left-color:#000;
  transform:rotate(135deg);
  margin-left:10rpx;
}

.s-entrance-list {
  width: 100%;
  box-sizing:border-box;
  padding:17rpx 30rpx 11rpx;
  display:flex;
  flex-wrap:wrap;
}

.s-entrance-item {
  width: 20%;
  display:flex;
  flex-direction:column;
  align-items:center;
  margin-bottom:10rpx;
}

.s-entrance-ico {
  width: 120rpx;
  height:120rpx;
  border-radius:3px;
} 

.s-entrance-name {
  width: 100%;
  height:58rpx;
  line-height:58rpx;
  font-size: 26rpx;
  color:#6d6d6d;
  text-align:center;
}
