.s-module {
  width: 100%;
  background:#fff;
  margin-bottom:20rpx;
}

.s-list-head {
  width: 100%;
  height: 100rpx;
  line-height:100rpx;
  border-bottom:1px solid #e3e3e3;
  box-sizing:border-box;
  padding:0 30rpx;
  font-size: 28rpx;
  color:#000;
}

.s-list-list {
  width: 100%;
  display:flex;
  justify-content:space-between;
  flex-wrap: wrap;
  box-sizing:border-box;
  padding:0 40rpx;
}

.s-list-item {
  width: 33.333%;
  height: 160rpx;
  display:flex;
  flex-direction:column;
  align-items:center;
  box-sizing:border-box;
  padding-top:30rpx;
}

.s-list-item image {
  width: 46rpx;
  height: 46rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-list-item text {
  height: 58rpx;
  line-height: 58rpx;
  font-size: 26rpx;
  color:#000;
}

.s-my-list {
  width: 730rpx;
  border-radius:10rpx;
  background:#fff;
  margin:0 auto;
  display:flex;
  flex-wrap:wrap;
  padding-top:20rpx;
  justify-content: flex-start;
}

.s-my-item {
  width: 33.3%;
  height:150rpx;
  font-size:26rpx;
  display:flex;
  flex-direction:column;
  align-items:center;
  box-sizing:border-box;
  justify-content:center;
  background:#fff;
  padding:0;
}

.s-my-ico {
  width: 50rpx;
  height: 50rpx;
  margin-bottom:10rpx;
}

.s-my-title {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  text-align:center;
}

.s-my-ico0, .s-share-ico0 {
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-my-ico1, .s-share-ico1 {
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-my-ico2 {
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-my-ico3, .s-my-ico4 {
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}
