.s-near {
  width: 100%;
  background:#fff;
}

.s-near-head {
  width: 100%;
  height:78rpx;
  line-height:78rpx;
  display:flex;
  align-content:center;
  justify-content:space-between;
  font-size:30rpx;
  box-sizing:border-box;
  padding:0 30rpx;
}

.s-near-head-more {
  display:flex;
  align-items:center;
  font-size:24rpx;
}

.s-near-head-more:after {
  content:" ";
  width: 10rpx;
  height:10rpx;
  border:1px solid transparent;
  border-top-color:#000;
  border-left-color:#000;
  transform:rotate(135deg);
  margin-left:10rpx;
}

.s-tabbar {
  width: 100%;
  height: 62rpx;
  display:flex;
  align-items:center;
  justify-content:space-between;
  box-sizing:border-box;
  padding:0 30rpx;
}

.s-tabbar-item {
  height: 100%;
  line-height:62rpx;
  font-size:28rpx;
}

.s-near-list {
  width: 100%;
}

.s-near-item {
  width: 100%;
  height:250rpx;
  box-sizing:border-box;
  border-bottom:1px solid #eeeeee;
  padding: 27rpx 30rpx;
  display:flex;
  justify-content:space-between;
}

.s-ad-item {
  padding:0;
  display:flex;
  justify-content:center;
  border:none;
}

.s-ad-img {
  width: 700rpx;
  height: 100%;
}

.s-near-img {
  width: 214rpx;
  height: 171rpx;
}

.s-near-info {
  width: 440rpx;
}

.s-near-title {
  width: 100%;
  height: 40rpx;
  line-height:40rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:30rpx;
  color:#080808;
  font-weight:bold;
  margin-bottom:20rpx;
}

.s-near-address {
  width: 100%;
  height: 60rpx;
  color:#676767;
  display:flex;
  justify-content:space-between;
  align-items:center;
  font-size: 28rpx;
  overflow: hidden;
}

.s-near-b {
  width: 100%;
  height: 42rpx;
  display:flex;
  align-items:center;
  justify-content:space-between;
  overflow: hidden;
}

.s-near-address-text {
  width: 320rpx;
  height: 100%;
  display:flex;
  align-items:center;
  justify-content: space-between;
}

.s-near-address-distance {
  width: 140rpx;
  height: 100%;
  font-size: 24rpx;
  display:flex;
  align-items:center;
  justify-content: flex-end;
}

.s-near-address-distance:before {
  width: 28rpx;
  height: 28rpx;
  content: " ";
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right: 10rpx;
}

.s-near-address-text text {
  width: 270rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.s-near-address-text:before {
  content:" ";
  width: 28rpx;
  height: 28rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right:10rpx;
}

.s-near-desc {
  width: 350rpx;
  font-size:20rpx;
  color:#828282;
  display:flex;
  align-items:center;
}

.s-near-desc text {
  width: 300rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.s-near-desc:before {
  content:" ";
  width: 28rpx;
  height: 28rpx;
  background:url('data:image/png;base64,') no-repeat;
  background-size:cover;
  margin-right:10rpx;
}


.s-near-desc2 {
  width: 110rpx;
  font-size:24rpx;
  text-align:right;
  color:#828282;
}

.s-active {
  color:#fb6464;
  font-weight:bold;
}

.s-near-head-title {
  font-weight:bold;
}