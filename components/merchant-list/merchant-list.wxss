.s-near {
  width: 100%;
  background:#fff;
}

.s-near-head {
  width: 100%;
  height:78rpx;
  line-height:78rpx;
  display:flex;
  align-content:center;
  justify-content:space-between;
  font-size:30rpx;
  box-sizing:border-box;
  padding:0 30rpx;
}

.s-near-head-more {
  display:flex;
  align-items:center;
  font-size:24rpx;
}

.s-near-head-more:after {
  content:" ";
  width: 10rpx;
  height:10rpx;
  border:1px solid transparent;
  border-top-color:#000;
  border-left-color:#000;
  transform:rotate(135deg);
  margin-left:10rpx;
}

.s-tabbar {
  width: 100%;
  height: 80rpx;
  display:flex;
  align-items:center;
  justify-content:space-between;
  box-sizing:border-box;
  padding:0 30rpx;
  border-bottom:1px solid #f5f5f5;
}

.s-tabbar-item {
  height: 100%;
  line-height:80rpx;
  font-size:28rpx;
  display:flex;
  align-items:center;
  justify-content:center;
}

.s-view {
  width: 100%;
  height:100%;
  background:rgba(0,0,0,.3);
  position:absolute;
  left: 0;
  top: 80rpx;
}

.s-view-list {
  width: 100%;
  background:#fff;
}

.s-view-item i {
  width: 28rpx;
  height:28rpx;
  margin-right:15rpx;
}

.s-view-list1 {
  box-sizing:border-box;
  padding:20rpx;
  display:flex;
}

.s-view-item1 {
  padding: 0 15rpx;
  border:1px solid #ccc;
  height: 40rpx;
  line-height: 40rpx;
  font-size:24rpx;
  display:inline-block;
  border-radius: 10rpx;
  margin:0 10rpx;
  margin-bottom:10rpx;
}

.s-view-list-ico {
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-view-item {
  width: 100%;
  height: 80rpx;
  box-sizing:border-box;
  border-bottom:1px solid #f3f3f3;
  padding:0 20rpx;
  font-size:28rpx;
  display:flex;
  align-items:center;
  color:#4e4e4e;
}

.s-view-item-active {
  color:#fb6464;
}

.s-near-list {
  width: 100%;
}

.s-near-item {
  width: 100%;
  height:250rpx;
  box-sizing:border-box;
  border-bottom:1px solid #eeeeee;
  padding: 27rpx 30rpx;
  display:flex;
  justify-content:space-between;
}

.s-near-img {
  width: 200rpx;
  height: 200rpx;
  border-radius:4px;
}

.s-near-info {
  width: 66.5%;
}

.s-near-title {
  width: 100%;
  height: 50rpx;
  line-height:50rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:30rpx;
  color:#080808;
  margin-bottom:5rpx;
  font-weight:bold;
}

.s-near-address {
  width: 100%;
  height: 48rpx;
  line-height:48rpx;
  font-size: 28rpx;
  color:#5d5d5d;
  display:flex;
  justify-content:space-between;
  margin-bottom:15rpx;
  align-items:center;
}

.s-near-address-ico {
  width: 24rpx;
  height: 26rpx;
  background:url('data:image/png;base64,');
  background-size:cover;
}

.s-near-address-text {
  width: 65%;
  height: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.s-near-address-distance {
  width: 25%;
  height: 100%;
  overflow:hidden;
  text-align:right;
}

.s-near-desc-wrap {
  width: 100%;
  display:flex;
  justify-content:space-between;
}

.s-near-desc {
  width: 92%;
  line-height:32rpx;
  font-size:24rpx;
  color:#565656;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

.s-near-desc-ico {
  width: 24rpx;
  height: 26rpx;
  background:url('data:image/png;base64,');
  background-size:cover;
  display:inline-block;
  margin-top:4rpx;
}

.active {
  color:#fb6464;

}

.s-tabbar-item .s-tabbar-ico {
  width: 20rpx;
  height: 20rpx;
  margin-left: 10rpx;
}

.active .s-tabbar-ico {
  width:0;
  height:0;
  margin-top:-4rpx;
  border:8rpx solid transparent;
  transform:rotate(-135deg);
  border-top-color:#fb6464;
  border-left-color:#fb6464;
}