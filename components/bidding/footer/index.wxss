.s-footer {
  width: 100%;
  height: 100rpx;
  background:#ffffff;
  position:fixed;
  left:0;
  bottom:0;
  display:flex;
  box-sizing:border-box;
  border-top:1px solid #eeeeee;
}

.s-footer-l {
  width: 164rpx;
  height: 100%;
  border-right:1px solid #f5aaa4;
  display:flex;
  align-items:center;
  justify-content:center;
  font-size: 28rpx;
  color:#6b6b6b;
}

.s-footer-l:before {
  content: " ";
  width: 38rpx;
  height: 38rpx;
  margin-right:1px;
  background:url('data:image/png;base64,');
  background-size:cover;
}

.s-footer-c {
  width: 449rpx;
  height: 100%;
  display:flex;
  align-items:center;
  justify-content:center;
}

.s-footer-c-shift {
  width: 95rpx;
  height: 65rpx;
  box-sizing:border-box;
  border:1px solid #f8aba1;
  font-size:38rpx;
  color:#666666;
}

.s-footer-c-shift, .s-footer-c-unshift, .s-footer-c-input {
  display:flex;
  align-items:center;
  justify-content:center;
}

.s-footer-c-unshift {
  width: 103rpx;
  height: 65rpx;
  box-sizing:border-box;
  border:1px solid #f8aba1;
  font-size:38rpx;
  color:#666666;
}

.s-footer-c-input {
  width: 180rpx;
  height: 65rpx;
  box-sizing:border-box;
  border-top:1px solid #f8aba1;
  border-bottom:1px solid #f8aba1;
  font-size: 30rpx;
  color:#000000;
}

.s-footer-r {
  width: 137rpx;
  height: 100%;
  background:#ed5749;
  font-size:30rpx;
  color:#fff;
  display:flex;
  align-items:center;
  justify-content:center;
}