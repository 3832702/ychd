.s-categories {
  width: 100%;
  background:#fff;
  margin-bottom:20rpx;
}

.categories-title {
  width: 100%;
  height: 126rpx;
  line-height: 126rpx;
  font-weight:bold;
  font-size: 30rpx;
  box-sizing:border-box;
  padding: 0 30rpx;
  color:#000000;
}

.categories-list {
  width: 100%;
  display:flex;
  flex-wrap:wrap;
}

.categories-item {
  width: 25%;
  height: 172rpx;
  font-size: 28rpx;
  color:#666666;
  display:flex;
  flex-direction:column;
  align-items:center;
  box-sizing:border-box;
  padding-top:8rpx;
  overflow:hidden;
  margin:0;
  background:#fff;
  position:relative;
}

.categories-item-title2 {
  width: 100%;
  height: 30rpx;
  font-size: 26rpx;
  position:absolute;
  left:0;
  bottom:25rpx;
  color:#fb6464;
}

.categories-item-ico {
  width: 50rpx;
  height: 50rpx;
}

.categories-item-title {
  height: 70rpx;
  line-height: 70rpx;
}

.s-share-ico {
  background:url('data:image/png;base64,') no-repeat;
  background-size:cover;
}