.s-recently {
  width: 100%;
  background:#fff;
  border-top:1px solid #e1e1e1;
  border-bottom:1px solid #e1e1e1;
}

.s-recently-head {
  width: 100%;
  height: 70rpx;
  display:flex;
  align-items:center;
  box-sizing:border-box;
  padding:0 25rpx;
  font-size:30rpx;
  color:#989898;
  margin-bottom:10rpx;
}

.s-recently-head:before {
  content: " ";
  width: 30rpx;
  height: 30rpx;
  margin-right:12rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;

}

.s-recently-list {
  width: 100%;
}

.s-recently-item {
  width: 100%;
  height: 90rpx;
  box-sizing:border-box;
  padding:0 20rpx;
  font-size:26rpx;
  display:flex;
  align-items:center;
  justify-content:space-between;
  color:#979797;
  position:relative;
}

.s-recently-item:after {
  content: "";
  width: 34rpx;
  height: 34rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right:10rpx;
}

.s-recently-item:before {
  content: "";
  height: 1px;
  background:#ececec;
  position:absolute;
  bottom:0;
  left: 20rpx;
  right:0;
}

.s-recently-item:last-child:before {
  background:transparent; 
}