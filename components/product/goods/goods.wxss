.s-goods {
  width: 100%;
  background:#ffffff;
  margin-bottom:20rpx;
}

.s-goods-img {
  width: 100%;
  height: 600rpx;
}

.s-goods-img-wrap {
  width: 100%;
  position:relative;
}

.s-goods-img-1 {
  width: 750rpx;
  height: 600rpx;
}

.s-goods-info {
  width: 100%;
  height: 90rpx;
  display:flex;
  box-sizing:border-box;
  padding-left: 30rpx;
  align-items:center;
  border-bottom:1rpx solid #f5f5f5;
}

.s-goods-title {
  max-width: 350rpx;
  font-size:34rpx;
  font-weight:bold;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-right:30rpx;
}

.s-goods-stars {
  display:flex;
}

.s-goods-stars-item {
  width: 28rpx;
  height:28rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right:6rpx;
}

.s-goods-collection {
  width: 150rpx;
  height: 50rpx;
  font-size: 28rpx;
  color:#9f9f9f;
  line-height:50rpx;
  margin-left:auto;
  display:flex;
  align-items:center;
  background:#f4f4f4;
  justify-content:center;
  border-radius: 25rpx 0 0 25rpx;
  overflow:hidden;
}

.s-goods-button, .s-goods-button1 {
  width: 100%;
  height: 100%;
  font-size: 26rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  padding:0;
  background:transparent;
}

.s-goods-button:before {
  content: " ";
  width:28rpx;
  height:28rpx;
  margin-right:10rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-goods-button1 {
  color:#fb6464;
}

.s-goods-button1:before {
  content: " ";
  width:28rpx;
  height:28rpx;
  margin-right:10rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}



.s-goods-share {
  width: 130rpx;
  margin-left:auto;
  background:#f4f4f4;
  border-radius: 25rpx 0 0 25rpx;
  height:50rpx;
  display:flex;
  align-items:center;
  font-size:26rpx;
  color:#9f9f9f;
  justify-content:center;
  position:fixed;
  right:0;
  top: 20rpx;
  z-index:1111111;
}

.s-goods-more {
  width: 100%;
  height:150rpx;
  background:#ffffff;
  box-sizing:border-box;
  padding:0 30rpx;
  display:flex;
  justify-content:space-between;
  font-size:26rpx;
  color:#fff;
}

.s-goods-price {
  width: 459rpx;
  display:flex;
  position:relative;
  box-sizing:border-box;
  padding:14rpx 0;
  flex-direction: column;
}

.s-goods-price-l {
  height: 100%;
  display:flex;
  flex-direction:column;
}

.s-goods-price:after {
  content: " ";
  width: 1px;
  height: 120rpx;
  background:#f5f5f5;
  position:absolute;
  right:0;
  top:16rpx;
}

.s-goods-price-1 {
  width: 100%;
  height:46rpx;
  margin-top:6rpx;
  font-size: 46rpx;
  color:#fc6463;
  vertical-align:top;
}

.s-goods-price-1 text {
  font-size: 30rpx;
  margin-left:20rpx;
}

.s-goods-price-1:before {
  content: "到店体验价";
  font-size: 30rpx;
  line-height:60rpx;
  height:60rpx;
  color:#4d4d4d;
  display:inline-block;
  vertical-align:bottom;
}

.s-goods-price-2 {
  width: 100%;
  height:60rpx;
  line-height:60rpx;
  text-decoration:line-through;
  color:#6b6b6b;
}

.s-goods-price-sub {
  display:flex;
  align-items:center;
  font-size:56rpx;
  height: 56rpx;
}

.s-goods-price-sub text {
  font-size: 30rpx;
  margin-left:60rpx;
  align-self:flex-end;
}

.s-goods-time {
  width: 198rpx;
  line-height:50rpx;
  align-self:center;
  display:flex;
  flex-direction:column;
  box-sizing:border-box;
  padding: 14rpx 0;
  color:#4d4d4d;
}

.s-goods-time-1 {
  background:#fb6464;
  color:#fff;
  width: 170rpx;
  text-align:center;
  font-size:28rpx;
}

.s-goods-time-2 {
  width: 170rpx;
  box-sizing:border-box;
  border:1px solid #fb6464;
  text-align:center;
}

.s-banner-prompt {
  width: 100%;
  height: 60rpx;
  position:absolute;
  left:0;
  bottom:0;
  background:transparent;
  text-align:center;
  line-height: 60rpx;
  font-size: 30rpx;
  color:#fb6464;
  background:rgba(0,0,0,.2);
}

.s-goods-shop {
  width: 100%;
  height: 80rpx;
  background:#fff;
  margin-bottom:20rpx;
  box-sizing:border-box;
  padding:0 35rpx;
  display:flex;
  align-items:center;
}

.s-goods-avatar {
  width: 60rpx;
  height: 60rpx;
  border-radius:50%;
  margin-right:20rpx;
}

.s-goods-shop-title {
  width: 420rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:28rpx;
  color:#000000;
}

.s-goods-shop-more {
  font-size:26rpx;
  color:#989898;
  margin-left:auto;
  display:flex;
  align-items:center;
  height:100%;
}

.s-goods-shop-more:after {
  content: " ";
  width: 10rpx;
  height:10rpx;
  border:1px solid transparent;
  border-top-color:#999;
  border-left-color:#999;
  transform:rotate(135deg);
  margin-left:10rpx;
}