.s-list-wrap {
  width:730rpx;
  background:#fff;
  position:relative;
  margin:0 auto;
  border-radius:10rpx;
  padding-top:20rpx;
}

.s-list-head {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  color:#666;
  display:flex;
  align-items:center;
  justify-content:center;
  position:relative;
}

.s-list-head:after {
  content: " ";
  width: 50rpx;
  height: 2rpx;
  background:#ccc;
  margin-left: 20rpx;
}

.s-list-head:before {
  content: " ";
  width: 50rpx;
  height: 2rpx;
  background:#ccc;
  margin-right:20rpx;
}

.s-listbar {
  width: 100%;
  display:flex;
  flex-wrap:wrap;
  box-sizing:border-box;
  padding:0 20rpx;
}

.s-listbar-item {
  width: 50%;
  box-sizing:border-box;
  display:flex;
  flex-direction:column;
  padding-top:25rpx;
}

.s-listbar-item:nth-of-type(2n) {
  align-items:flex-end;
}

.s-listbar-img {
  width: 335rpx;
  height: 268rpx;
  margin-bottom:15rpx;
  border-radius:10rpx;
}

.s-listbar-title {
  width: 335rpx;
  height:60rpx;
  line-height:60rpx;
  font-size: 28rpx;
  color:#000000;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  text-align:center;
}

.s-listbar-price {
  width: 335rpx;
  height: 24rpx;
  line-height:24rpx;
  font-size:24rpx;
  color:#fc6463;
  text-align:center;
}

.s-no {
  width: 100%;
  height: 200rpx;
  display:flex;
  justify-content:center;
  align-items:center;
  color:#ccc;
  font-size: 28rpx;
}