.s-tabbar {
  width: 100%;
  background:#fff;
  margin-bottom:20rpx;
  padding:15rpx 0;
}

.s-tabbar-t {
  width: 100%;
  display:flex;
  align-items:center;
  position:relative;
  overflow:hidden;
}

.s-tabbar-t:before {
  content: " ";
  height:1rpx;
  background:#ccc;
  position:absolute;
  left:30rpx;
  right:0;
  bottom:0;
}

.s-tabbar-desc {
  width: 78%;
  font-size:26rpx;
  color:#999;
  box-sizing:border-box;
  padding:0 30rpx;
}

.s-tabbar-desc-1 {
  display:flex;
  padding:10rpx 30rpx;
  overflow:hidden;
}

.s-tabbar-address {
  width: 100%;
  height: 70rpx;
  color:#999;
  font-size: 26rpx;
  box-sizing:border-box;
  padding:0 30rpx;
  line-height:78rpx;
  display:flex;
  align-items:center;
}

.s-tabbar-address text {
  width: 460rpx;
}



.s-tabbar-desc-sub {
  display:inline-block;
  width: 390rpx;
  line-height: 40rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  margin-left:20rpx;
}

.s-tabbar-address-ico {
  width: 34rpx;
  height: 34rpx;
  content: " ";
  background:url("data:image/png;base64,");
  background-size:cover;
  margin-right:15rpx;
}

.s-tabbar-list {
  width: 100%;
  display:flex;
  align-items:center;
  justify-content:space-between;
  height:88rpx;
  box-sizing:border-box;
  padding: 0 30rpx;
  position:fixed;
  left: 120rpx;
  z-index:11111111;
}

.s-tabbar-item {
  font-size:30rpx;
  color:#fff;
}

.s-tabbar-focus {
  width: 150rpx;
  height: 50rpx;
  background:#fb6464;
  border-radius:30rpx;
  font-size: 26rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  color:#fff;
}

.s-tabbar-focus1 {
  background:#fb6464;
  width: 150rpx;
  height: 50rpx;
  border-radius:30rpx;
  font-size: 26rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  color:#fff;
}

.s-tabbar-search {
  width: 50rpx;
  display:flex;
  height:50rpx;
  align-items:center;
  justify-content:center;
  border-radius:25rpx;
  color:#fff;
}

.s-tabbar-search:before {
  width: 30rpx;
  height: 30rpx;
  content: " ";
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right:5rpx;
}


.s-info {
  width: 100%;
  height: 254rpx;
  background:#fff;
  margin-bottom:38rpx;
  display:flex;
  justify-content:space-between;
  box-sizing:border-box;
  padding:0 30rpx;
}

.s-avatar-l {
  width: 150rpx;
  padding-top:11rpx;
}

.s-avatar {
  width: 150rpx;
  height: 150rpx;
  border-radius:50%;
  background:#f1f3f0;
  display:flex;
  align-items:center;
  justify-content:center;
}

.s-avatar image {
  width: 140rpx;
  height: 140rpx;
  border-radius:50%;
}


.s-stars {
  height: 32rpx;
  display:flex;
  width: 100%;
  align-items:center;
  justify-content:space-between;
}

.s-stars-item {
  width: 20rpx;
  height: 20rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-fans {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 22rpx;
  color:#adadad;
}

.s-avatar-r {
  width: 505rpx;
  padding-top:13rpx;
}

.s-info-title {
  width: 100%;
  height: 55rpx;
  line-height: 55rpx;
  display:flex;
  align-items:center;
  justify-content: space-between;
}

.s-info-title text {
  width: 66%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:34rpx;
  font-weight:bold;
}

.s-info-time {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color:#adadad;
}

.s-info-desc {
  height: 62rpx;
  width: 100%;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
  color:#adadad;
  font-size: 24rpx;
  line-height: 31rpx;
}

.s-info-address {
  width: 100%;
  height: 50rpx;
  display:flex;
  align-items:center;
  font-size: 24rpx;
  color:#adadad;
  justify-content:space-between;
}

.s-tabbar-desc-text {
  width: 430rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}