.s-footer {
  width: 100%;
  height:100rpx;
  position:fixed;
  left:0;
  bottom:0;
  background:#f9f9f9;
  display:flex;
  align-items:center;
  justify-content:space-around;
}

.s-footer-item {
  display:flex;
  flex-direction:column;
  align-items:center;
  background:transparent;
  margin:0;
  background:transparent;
}

.s-footer-avatar {
  width: 80rpx;
  height:80rpx;
  border-radius:50%;
  background:#ccc;
}

.s-footer-ico {
  width: 45rpx;
  height:45rpx;
  background:#dbdbdb;
  margin-top:10rpx;
}

.s-footer-title {
  width: 100%;
  height: 44rpx;
  line-height:44rpx;
  font-size:24rpx;
  color:#4d4d4d;
}

