page {
  background:#f5f5f5;
}

.s-list {
  width: 100%;
}

.s-item {
  width: 100%;
  background:#fff;
  margin-bottom:20rpx;
}

.s-item-t {
  width: 100%;
  box-sizing:border-box;
  border-bottom:1px solid #eeeeee;
  padding: 15rpx 30rpx 10rpx;
  display:flex;
  justify-content:space-between;
}

.s-img {
  width: 258rpx;
  height: 195rpx;
  border-radius:4px;
}

.s-info {
  width: 57.5%;
}

.s-title {
  width: 100%;
  height: 50rpx;
  line-height:50rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:26rpx;
  color:#080808;
  margin-bottom:5rpx;
}

.s-address {
  width: 100%;
  height: 28rpx;
  line-height:28rpx;
  font-size: 24rpx;
  color:#5d5d5d;
  display:flex;
  justify-content:space-between;
  margin-bottom:15rpx;
}

.s-address-text {
  width: 75%;
  height: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.s-address-distance {
  width: 25%;
  height: 100%;
  overflow:hidden;
  text-align:right;
}

.s-desc {
  width: 100%;
  line-height:34rpx;
  font-size:24rpx;
  color:#ccc;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display:block;
}

.s-desc-price {
  height: 40rpx;
  color:#fb6464;
  font-size: 30rpx;
}

.s-not {
  width:100%;
  height: 300rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  font-size:30rpx;
  color:#ccc;
}