page {
  background:#f5f5f5;
}

.s-search {
  width: 100%;
  height: 92rpx;
  background:#fff;
  display:flex;
  align-items:center;
  box-sizing:border-box;
  padding:0 20rpx;
  justify-content:space-between;
  border-bottom:1px solid #e1e1e1;
  margin-bottom:20rpx;
}

.s-search-text {
  width: 600rpx;
  height: 56rpx;
  background:#f2f2f2;
  border-radius:10rpx;
  box-sizing:border-box;
  padding: 0 0 0 20rpx;
  font-size:26rpx;
}

.s-search-btn {
  min-width:94rpx;
  height: 56rpx;
  background:#fb6464;
  line-height: 56rpx;
  text-align:center;
  color:#fff;
  font-size:28rpx;
  border-radius:10rpx;
  border:none;
}

.s-not {
  width:100%;
  height: 300rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  font-size:30rpx;
  color:#ccc;
}