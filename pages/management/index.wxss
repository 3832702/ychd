page {
  background:#f4f4f4;
}
.s-management {
  width: 100%;
  box-sizing:border-box;
}

.s-banner {
  width: 100%;
  height: 150rpx;
}

.s-banner-img {
  width: 100%;
  height: 100%;
}

.s-avatar {
  width: 100%;
  height: 190rpx;
  border-bottom:1px solid #e6e6e6;
  display:flex;
  align-items:center;
  box-sizing:border-box;
  padding:0 30rpx;
  background:#fff;
}

.s-avatar-text {
  font-size: 30rpx;
  color:#000;
  margin-right:100rpx;
}

.s-avatar-img {
  width: 132rpx;
  height: 132rpx;
  position:relative;
}

.s-avatar-title {
  width: 100%;
  height: 30rpx;
  font-size: 22rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  position:absolute;
  left:0;
  bottom:0;
  background:rgba(0,0,0,.3);
  color:#fff;
}

.s-avatar-img image {
  width: 132rpx;
  height: 132rpx;
}

.s-name {
  width: 100%;
  height: 90rpx;
  background:#fff;
  box-sizing:border-box;
  padding:0 30rpx;
  display:flex;
  align-items:center;
  margin-bottom:20rpx;
}

.s-name-input {
  width: 320rpx;
  height: 100%;
  line-height: 90rpx;
  font-size:26rpx;
}

.s-address {
  width: 100%;
  height:90rpx;
  background:#fff;
  display:flex;
  align-items:center;
  box-sizing:border-box;
  padding:0 30rpx;
  border-bottom:1px solid #f3f3f3;
}

.s-address-location {
  width: 132rpx;
  height: 46rpx;
  line-height: 46rpx;
  border-radius:23rpx;
  font-size: 24rpx;
  color:#fff;
  background:#f77a06;
  text-align:center;
  margin-left:auto;
}

.s-timer {
  width: 100%;
  height: 90rpx;
  display:flex;
  align-items:center;
  box-sizing:border-box;
  padding:0 30rpx;
  background:#fff;
  margin-bottom:20rpx;
}

.s-timer .s-name-input {
  width: 400rpx;
}

.s-desc {
  width: 100%;
  height:260rpx;
  display:flex;
  background:#fff;
  align-items:center;
  box-sizing:border-box;
  padding:0 30rpx;
  margin-bottom:20rpx;
  position:relative;
}

.s-desc-num {
  position:absolute;
  right:100rpx;
  bottom:40rpx;
  font-size: 26rpx;
  color:#ccc;
}

.s-avatar-textarea {
  width: 450rpx;
  height: 220rpx;
  font-size:26rpx;
  line-height:40rpx;
  box-sizing:border-box;
  padding:20rpx 10rpx;
  border:1px solid #ccc;
  border-radius:10rpx;
}

.s-img {
  width: 100%;
  min-height: 308rpx;
  background:#fff;
  box-sizing:border-box;
  padding:0 30rpx 100rpx;
  display:flex;
}

.s-img .s-avatar-text {
  height: 92rpx;
  line-height:92rpx;
}

.s-img-view {
  width: 460rpx;
}

.s-img-prompt {
  width: 100%;
  height: 92rpx;
  line-height:92rpx;
  color:#949494;
  font-size: 24rpx;
}

.s-img-list {
  width: 100%;
  display:flex;
  flex-wrap:wrap;
  overflow:hidden;
  padding-top:20rpx;
}

.s-img-item {
  width: 130rpx;
  height: 130rpx;
  margin:0 5rpx 20rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-position: 50% 50%;
  border:1px solid #dbdbdb;
}

.s-img-item image {
  width: 100%;
  height: 100%;
}

.s-img-itemimg {
  background:transparent;
  position:relative;
  border-color:transparent;
}

.s-img-remove {
  width: 40rpx;
  height: 40rpx;
  background:#ccc;
  position:absolute;
  top:0rpx;
  right:0rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-submit {
  width: 100%;
  height: 100rpx;
  position:fixed;
  left:0;
  bottom:0;
  background:#fb6464;
  color:#fff;
  text-align:center;
  line-height:100rpx;
  font-size: 32rpx;
}

.picker {
  font-size: 26rpx;
}

.click-ccc {
  font-size: 24rpx;
  color:#949494;
  margin-left: 20rpx;
}