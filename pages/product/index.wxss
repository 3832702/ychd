page {
  background:#f2f2f2;
  box-sizing:border-box;
  padding-bottom:100rpx;
}

.s-ad {
  width: 730rpx;
  height: 250rpx;
  margin:50rpx auto 0;
  margin-bottom:80rpx;
}

.s-ad-head {
  width:100%;
  font-size: 26rpx;
  height: 40rpx;
  text-align:center;
  display:flex;
  align-items:center;
  justify-content:center;
  color:#ccc;
  margin-bottom:20rpx;
}

.s-ad-head:after {
  content: " ";
  width: 50rpx;
  height: 1px;
  background:#ccc;
  margin-left: 30rpx;
}

.s-ad-head:before {
  content: " ";
  width: 50rpx;
  height: 1px;
  background:#ccc;
  margin-right: 30rpx;
}

.s-ad-img {
  width: 100%;
  height: 100%;
}

.s-back, .s-back-home {
  width: 64rpx;
  height: 64rpx;
  background:rgba(242, 243, 248, .3);
  position:fixed;
  left:30rpx;
  z-index:100000000;
  border-radius:50%;
  display:flex;
  justify-content:center;
  align-items:center;
}


.s-back:after {
  width: 12rpx;
  height: 12rpx;
  content: " ";
  border:1px solid transparent;
  border-top-color:#0e0f11;
  border-left-color:#0e0f11;
  transform:rotate(-45deg);
  margin-left:6rpx;
}


.s-back-home-ico {
  width: 40rpx;
  height: 40rpx;
  background:url('data:image/png;base64,') no-repeat;
  background-size:cover;
}