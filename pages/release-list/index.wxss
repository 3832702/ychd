page {
  background:#f5f5f5;
}

.s-tab {
  width: 100%;
  height: 100rpx;
  background:#fff;
  display:flex;
  align-items:center;
  border-bottom:1px solid #f5f5f5;
  position:fixed;
  top:0;
  left:0;
}

.s-tab-item {
  width: 33.333%;
  font-size: 30rpx;
  display:flex;
  height:100%;
  align-items:center;
  justify-content:center;
}

.s-tab-mask {
  width: 10%;
  height: 1px;
  background:#fb6464;
  position:absolute;
  bottom:0;
  transition:.3s;
}

.s-tab-item-active {
  color:#fb6464;
  transition:.5s;
}

.s-tab-mask1 {
  left:87rpx;
}

.s-tab-mask0 {
  left:337rpx;
}

.s-tab-mask2 {
  left:587rpx;
}


.s-tab-mask3 {
  left:618rpx;
}

.s-list {
  width: 100%;
  padding-top:100rpx;
}

.s-item {
  width: 100%;
  background:#fff;
  margin-bottom:20rpx;
}

.s-item-t {
  width: 100%;
  box-sizing:border-box;
  border-bottom:1px solid #eeeeee;
  padding: 15rpx 30rpx 10rpx;
  display:flex;
  justify-content:space-between;
}

.s-img {
  width: 258rpx;
  height: 195rpx;
  border-radius:4px;
}

.s-info {
  width: 57.5%;
}

.s-title {
  width: 100%;
  height: 50rpx;
  line-height:50rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size:26rpx;
  color:#080808;
  margin-bottom:5rpx;
}

.s-address {
  width: 100%;
  height: 28rpx;
  line-height:28rpx;
  font-size: 24rpx;
  color:#5d5d5d;
  display:flex;
  justify-content:space-between;
  margin-bottom:15rpx;
}

.s-address-text {
  width: 75%;
  height: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.s-address-distance {
  width: 25%;
  height: 100%;
  overflow:hidden;
  text-align:right;
}

.s-desc {
  width: 100%;
  line-height:35rpx;
  font-size:22rpx;
  color:#ccc;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display:block;
}

.s-desc-price {
  height: 40rpx;
  color:#fb6464;
  font-size: 30rpx;
}

.s-more {
  width: 100%;
  height: 70rpx;
  display:flex;
  box-sizing:border-box;
  padding:0 30rpx;
  align-items:center;
  font-size: 26rpx;
  
}

.s-more-shelves, .s-more-rm, .s-more-edit, .s-more-shelves-wrap {
  width: 25%;
  display:flex;
  height: 100%;
  align-items:center;
  justify-content:center;
}

.s-more-shelves1, .s-more-shelves {
  width: 100%;
  display:flex;
  height: 100%;
  align-items:center;
  justify-content:center;
}

.s-more-edit:before {
  content: " ";
  width: 32rpx;
  height: 32rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right:10rpx;
}

.s-more-rm:before {
  content: " ";
  width: 32rpx;
  height: 32rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right:10rpx;
}


.s-more-shelves:before {
  content: " ";
  width: 32rpx;
  height: 32rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right:10rpx;
}

.s-more-shelves1:before {
  content: " ";
  width: 32rpx;
  height: 32rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
  margin-right:10rpx;
}

.s-not {
  width:100%;
  height: 300rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  font-size:30rpx;
  color:#ccc;
}