page {
  background:#f4f4f4;
}

.s-release-head {
  width: 100%;
  height:126rpx;
  background:#fff;
  box-sizing:border-box;
  padding:0 30rpx;
  display:flex;
  align-items:center;
  font-size:38rpx;
  border-bottom:1px solid #f5f3f8;
  color:#000;
}

.s-avatar {
  width: 100%;
  height: 290rpx;
  border-bottom:1px solid #f5f3f8;
  display:flex;
  align-items:center;
  box-sizing:border-box;
  padding:0 30rpx;
  background:#fff;
}

.s-avatar-text {
  font-size: 30rpx;
  margin-right:100rpx;
}

.s-avatar-img {
  width: 162rpx;
  height: 130rpx;
  position:relative;
}

.s-avatar-title {
  width: 100%;
  height: 30rpx;
  font-size: 24rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  background:rgba(0,0,0,.3);
  color:#fff;
  position:absolute;
  left:0;
  top: 130rpx;
}

.s-add-img {
  display:flex;
  align-items:center;
  justify-content:center;
  background:#f2f2f2;
  border-radius:10rpx;
}

.s-add-img-ico {
  width: 90rpx;
  height: 90rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-avatar-img image {
  width: 162rpx;
  height: 130rpx;
}

.s-release {
  width: 100%;
}

.s-release-wrap {
  width: 100%;
  background:#fff;
  height:100rpx;
  display:flex;
  align-items:center;
  box-sizing:border-box;
  padding:0 30rpx;
  border-bottom:1px solid #f5f3f8;
}


.s-release-text {
  font-size: 30rpx;
  color:#666666;
  width: 230rpx;
}

.s-release-name {
  width: 300rpx;
  height: 100%;
  line-height: 90rpx;
  font-size: 26rpx;
}

.s-class {
  width: 100%;
  height:90rpx;
  box-sizing:border-box;
  padding:0 30rpx;
  display:flex;
  align-items:center;
  background:#fff;
  border-bottom:1px solid #f5f3f8;
}

.end-release {
  margin-bottom: 20rpx;
  height:44rpx;
  width: 100%;
  background:#fff;
}

.picker-wrap {
  width: 400rpx;
}

.picker {
  font-size: 26rpx;
  color:#333;
  width: 100%;
  height: 70rpx;
  line-height:70rpx;
}

.s-desc {
  width: 100%;
  background:#fff;
  padding-bottom:70rpx;
  overflow:hidden;
}

.s-desc-text {
  width: 100%;
  height: 60rpx;
  line-height:60rpx;
  font-size: 30rpx;
  box-sizing:border-box;
  padding:0 30rpx;
  border-bottom:1px solid #f4f4f4;
  display:block;
  margin-bottom:20rpx;
}

.s-desc-input-wrap {
  line-height: 40rpx;
  padding-top:40rpx;
}

.s-desc-input {
  width: 100%;
  height: 260rpx;
  box-sizing:border-box;
  padding:20rpx 64rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  margin-bottom:40rpx;
}

.s-desc-placeholder {
  line-height: 40rpx;
}


.s-img-list {
  width: 100%;
  display:flex;
  flex-wrap:wrap;
  overflow:hidden;
  padding: 0 59rpx;
}

.s-img-item {
  width: 196rpx;
  height: 196rpx;
  background:#f2f2f2;
  display:flex;
  align-items:center;
  justify-content:center;
  margin: 0 5rpx 10rpx;
}

.s-img-item .s-add-img-ico {
  width: 120rpx;
  height: 120rpx;
}

.s-img-item image {
  width: 100%;
  height: 100%;
}

.s-img-itemimg {
  background:transparent;
  position:relative;
  border-color:transparent;
}

.s-img-remove {
  width: 40rpx;
  height: 40rpx;
  background:#ccc;
  position:absolute;
  top:0rpx;
  right:0rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size:cover;
}

.s-prompt-1 {
  width: 690rpx;
  margin:0 auto;
  box-sizing:border-box;
  padding: 0 20rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  color:#ccc;
  margin-bottom:70rpx;
}

.s-submit {
  width:690rpx;
  height: 90rpx;
  margin:0 auto;
  background:#fb6464;
  color:#fff;
  line-height:90rpx;
  text-align:center;
  font-size:30rpx;
  border-radius:10rpx;
}